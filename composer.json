{
    "name": "kozo/liberation",
    "description": "It is a Laravel package for easy retrieval and execution of SQL files and more.",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework":"^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "orchestra/testbench": "^5.0|^6.0",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "Liberation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Liberation\\LiberationServiceProvider"
            ]
        }
    }
}
